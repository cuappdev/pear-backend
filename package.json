  "homepage": "https://github.com/cuappdev/pear-backend#readme",
  "dependencies": {
    "@types/cheerio": "^0.22.17",
    "@types/node-cron": "^2.0.3",
    "@types/swagger-ui-express": "^4.1.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "fuse.js": "^6.4.1",
    "google-auth-library": "^6.1.1",
    "node-cron": "^2.0.3",
    "pg": "^7.18.1",
